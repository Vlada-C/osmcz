    <script n:syntax="off">
      //utm_source=Newsletter+OSM+CZ&utm_campaign=613dacf067-OSMCZ_Jaro_2016&utm_medium=email&utm_term=0_81b42f6aac-613dacf067-45266529
    	function removeQuery (){
        if(!history.replaceState) return;

    		var q = location.search;
        q = q.replace(/_fid=[^&]+&?/, '');
